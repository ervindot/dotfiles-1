# Git
alias g='git'
alias gadd='git add'
alias gpush='git push origin master'

# List aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls CF'
alias l1='ls -1a'

# Dir aliases
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias -- -="cd -"                  # Go to previous dir with -
alias cd.='cd $(readlink -f .)' # Go to real dir (i.e. if current dir is linked)
