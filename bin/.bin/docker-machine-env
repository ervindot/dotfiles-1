#!/bin/bash

# Provisions docker env

if [ -x "$(command -v docker-machine)" > /dev/null ]
then
  # fetch the first running machine name
  local machine=$(docker-machine ls | grep "Running" | head -n1 | awk '{ print $1 }')
  if [ "$machine" != "" ];
  then
    eval "$(docker-machine env $machine)"
  fi
fi
